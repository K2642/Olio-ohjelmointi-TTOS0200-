using System;

namespace T10

{
    class Program
    {
        static void Main(string[] args)
        {
            int[] array = { 1, 2, 33, 44, 55, 68, 77, 96, 100 };
            for (int i=0; i < array.Length; i++)
            {
                if (array[i] % 2 ==0) Console.WriteLine("HEP Paikassa {0} parillinen luku: {1}", i, array[i]);
            }
            Console.ReadLine();
        }
    }
}
